#ifndef MARKER_DEF_LOGGING_DXX_GUARD
#define MARKER_DEF_LOGGING_DXX_GUARD

#ifdef DEBUG
    #define LOG_DEBUG(...) printf("\n[mker] " __VA_ARGS__);
#else
    #define LOG_DEBUG(...)
#endif

#define LOG_INFO(...)  printf("\n[mker] " __VA_ARGS__);
#define LOG_ERROR(...) fprintf(stderr, "\n" __VA_ARGS__);

#endif